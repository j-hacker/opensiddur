<?xml version="1.0" encoding="UTF-8"?>
<tei:TEI 
    xmlns:tei="http://www.tei-c.org/ns/1.0"
    xmlns:j="http://jewishliturgy.org/ns/jlptei/1.0"  
    xml:lang="en">
    <tei:teiHeader>
      <tei:publicationStmt>
        <tei:availability status="free">
          <tei:licence target="http://www.creativecommons.org/publicdomain/zero/1.0"/>
        </tei:availability> 
      </tei:publicationStmt>
    </tei:teiHeader>
    <tei:fsdDecl>
        <tei:fsDecl type="test:FS">
            <tei:fDecl name="DEFAULT_YES">
                <j:vSwitch type="yes-no"/>
                <tei:vDefault>
                    <j:yes/>
                </tei:vDefault>
            </tei:fDecl>
            <tei:fDecl name="DEFAULT_IF">
                <j:vSwitch type="yes-no"/>
                <tei:vDefault>
                    <tei:if>
                        <tei:fs type="test:FS">
                            <tei:f name="CONTROL"><j:no/></tei:f>
                        </tei:fs>
                        <tei:then/>
                        <j:yes/>
                    </tei:if>
                    <j:no/>     <!-- this is the else -->
                </tei:vDefault>
            </tei:fDecl>
        </tei:fsDecl>
    </tei:fsdDecl>
</tei:TEI>

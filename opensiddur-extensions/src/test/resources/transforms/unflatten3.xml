<tei:TEI xmlns:tei="http://www.tei-c.org/ns/1.0">
    <tei:text>
        <j:concurrent xmlns:j="http://jewishliturgy.org/ns/jlptei/1.0">
            <jf:merged xmlns:jf="http://jewishliturgy.org/ns/jlptei/flat/1.0">
                <div xmlns="http://www.tei-c.org/ns/1.0" jf:id="div1" jf:start="div1" jf:position="1"
                 jf:relative="-1"
                 jf:nlevels="1"
                 jf:nprecedents="1"
                 jf:layer-id="divlayer"
                 jf:nchildren="-1"/>
                <tei:seg jf:id="seg1" jf:stream="stream">one</tei:seg>
                <tei:div jf:suspend="div1" jf:position="1" jf:relative="1" jf:nlevels="-1"
                     jf:nprecedents="1"
                     jf:layer-id="divlayer"
                     jf:nchildren="1"/>
                <tei:seg jf:id="seg2" jf:stream="stream">two</tei:seg>
                <tei:div jf:continue="div1" jf:position="3" jf:relative="-1" jf:nlevels="1"
                     jf:nprecedents="1"
                     jf:layer-id="divlayer"
                     jf:nchildren="-1"/>
                <tei:seg jf:id="seg3" jf:stream="stream">three</tei:seg>
                <div xmlns="http://www.tei-c.org/ns/1.0" jf:end="div1" jf:position="3"
                 jf:relative="1"
                 jf:nlevels="-1"
                 jf:nprecedents="1"
                 jf:layer-id="divlayer"
                 jf:nchildren="1"/>
            </jf:merged>
        </j:concurrent>
    </tei:text>
</tei:TEI>
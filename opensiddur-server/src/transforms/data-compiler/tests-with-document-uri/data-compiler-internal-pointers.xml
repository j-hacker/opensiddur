<tei:TEI
  xmlns:tei="http://www.tei-c.org/ns/1.0"
  xmlns:j="http://jewishliturgy.org/ns/jlptei/1.0"
  xmlns:jx="http://jewishliturgy.org/ns/jlp-processor"
  xml:lang="en">
  <j:links>
    <tei:ptr jx:id="p1s1" target="#p1A"/>
    <tei:ptr jx:id="p1s2" target="#p1B"/>
  </j:links>
  <tei:teiHeader/>
  <tei:text>
    <jx:joined-concurrent>
      <tei:p jx:id="p1">
        <tei:s>
          <tei:ptr jx:id="p1A" target="#A"/>
        </tei:s>
        <tei:s>
          <tei:ptr jx:id="p1C" target="#C"/>
        </tei:s>
      </tei:p>
    </jx:joined-concurrent>
  </tei:text>
  <j:repository>
    <tei:seg jx:id="A">segment A</tei:seg>
    <tei:seg jx:id="B">segment B</tei:seg>
    <tei:seg jx:id="C">segment C</tei:seg>
  </j:repository>
</tei:TEI>

<tei:TEI xml:lang="en" xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:j="http://jewishliturgy.org/ns/jlptei/1.0"
  xmlns:jx="http://jewishliturgy.org/ns/jlp-processor">
  <tei:teiHeader/>
  <tei:text>
    <j:concurrent>
      <j:selection>
        <tei:ptr xml:id="sA" target="#rA"/>
        <tei:ptr xml:id="sB" target="#rB"/>
        <tei:ptr xml:id="sC" target="external-resource.xml#sA"/>
      </j:selection>
      <j:view type="p">
        <tei:p xml:id="p1">
          <tei:ptr target="#range(sA,sB)"/>
        </tei:p>
      </j:view>
    </j:concurrent>
    <j:repository>
      <tei:seg xml:id="rA">A</tei:seg>
      <tei:seg xml:id="rB">B</tei:seg>
    </j:repository>
  </tei:text>
</tei:TEI>


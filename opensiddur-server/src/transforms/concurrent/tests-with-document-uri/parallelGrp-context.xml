<tei:TEI xml:lang="en" xmlns:tei="http://www.tei-c.org/ns/1.0" xmlns:j="http://jewishliturgy.org/ns/jlptei/1.0"
  xmlns:jx="http://jewishliturgy.org/ns/jlp-processor"
  >
  <tei:teiHeader/>
  <tei:text>
    <j:concurrent>
      <tei:ptr type="selection" n="1" target="parallelGrp-original.xml#selection"/>
      <tei:ptr type="selection" n="2" target="parallelGrp-translation.xml#selection"/>
      <j:view type="parallelGrp">
        <j:parallelGrp>
          <j:original n="1" xml:base="parallelGrp-original.xml">
            <tei:ptr xml:id="orig-se1" target="#se1"/>
            <tei:ptr xml:id="orig-se2" target="#se2"/>
            <tei:ptr xml:id="orig-se3" target="#se3"/> 
          </j:original>
          <j:parallel n="2" xml:base="parallelGrp-translation.xml">
            <tei:ptr xml:id="par-se1" target="#se1"/>
            <tei:ptr xml:id="par-se2" target="#se2"/>
            <tei:ptr xml:id="par-se3" target="#se3"/> 
          </j:parallel>
        </j:parallelGrp>
      </j:view>
    </j:concurrent>
  </tei:text>
</tei:TEI>

